    4  apt-get update 
    5  apt-get upgrade 
    6  sudo apt install apt-transport-https ca-certificates curl software-properties-common
    7  curl -fsSL https://download.docker.com/linux/ubuntu/gpg | sudo gpg --dearmor -o /usr/share/keyrings/docker-archive-keyring.gpg
    8  echo "deb [arch=amd64 signed-by=/usr/share/keyrings/docker-archive-keyring.gpg] https://download.docker.com/linux/ubuntu focal stable" | sudo tee /etc/apt/sources.list.d/docker.list
    9  sudo apt update
   10  sudo apt install docker-ce
   11  sudo systemctl start docker
   12  sudo systemctl enable docker
   13  docker version

